[route=workflowDetail] {
	.details {
		ul {
			li {
				list-style-type: none;
				display: block;
			}
		}
		.key {
			font-weight: bold;
			width: 250px;
			display: inline-block;
			text-align: right;
			padding: 5px 15px;
		}
	}

	.log {
		width: 90%;
		margin: 20 auto;
		pre {
			padding: 10px;
			white-space: pre-wrap;
		}
	}
}