[route=workflowSearch] {
	.search-menu {
		.from, to {
			width: 175px;
		}
		text-align: center;
		div {
			margin: 20px 0px;
		}
		input, select {
			padding: 8px 12px;
			box-shadow: none;
		}
	}
	a.external {
		color: #a23636;
	}
	
	.limit {
		width: 100px;
		text-align: center;
		margin-right: 10px;
	}
	
	h1 {
		border-style: ${border/style} none ${border/style} none;
	}
}